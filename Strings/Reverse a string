def reverse_string(s):
    """
    Function to return the reversed version of the input string.
    
    Parameters:
    s (str): The input string to be reversed.
    
    Returns:
    str: The reversed string.
    """
    chars = list(s)
    i, j = 0, len(chars)-1
    while i<j:
        chars[i], chars[j] = chars[j], chars[i]
        i +=1 
        j -=1 
        
        
        
    return "".join(chars)
