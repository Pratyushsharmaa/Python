def merge_three_dictionaries(dict1, dict2, dict3):
    # Your code goes here
    merged = {}
    for key, value in dict1.items():
        merged[key] = value
        
    for key, value in dict2.items():
        merged[key] = value
        
    for key, value in dict3.items():
        merged[key] = value
        
    return merged
        
