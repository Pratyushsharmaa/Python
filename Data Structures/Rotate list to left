def rotate_left(ARR, D):
    """
    Function to rotate the list to the left by D positions.
    :param ARR: List[int] -> The list of integers
    :param D: int -> The number of positions to rotate
    :return: List[int] -> The list after rotation
    """
    if not ARR:
        return []
        
    D = D%len(ARR)
    
    for _ in range(D):
        first = ARR.pop(0)
        ARR.append(first)
        
    return ARR
    
